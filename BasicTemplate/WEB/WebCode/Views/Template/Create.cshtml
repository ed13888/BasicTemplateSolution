@{

}
@section style{
    <style>
        #qrcode {
            width: 256px;
            height: 256px;
        }
    </style>
}
<div class="container">
    <div class="row text-center my-6">
        <div id="qrcode" style="display:inline-block;"></div>
    </div>
    <div class="row text-center">
        <a href="@Html.Raw(ViewBag.Url)" target="_blank" class="btn btn-primary">预览</a>
    </div>
</div>

@section script{
    <script src="~/Plugins/jquery.qrcode/jquery.qrcode.min.js"></script>
    <script>
        $(function () {
            var url = "@Html.Raw(ViewBag.Url)";
            $("#qrcode").qrcode(url);
        })
    </script>
}